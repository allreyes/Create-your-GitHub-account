@model Instructor;

<h1>Instructor Details</h1>

<table class="table table-dark">
    <tr>
        <th>Id</th>
        <td>@Model.instructorID</td>
    </tr>
    <tr>
        <th>Last Name</th>
        <td>@Model.instructorLastName</td>
    </tr>
    <tr>
        <th>First Name</th>
        <td>@Model.instructorFirstName</td>
    </tr>
    <tr>
        <th>IsTenured</th>
        <td>@Model.instructorIsTenured</td>
    </tr>
    <tr>
        <th>Email</th>
        <td>@Model.instructorEmail</td>
    </tr>
    <tr>
        <th>Date Hired</th>
        <td>@Model.instructorDateHired</td>
    </tr>
    <tr>
        <th>Rank</th>
        <td>@Model.instructorRank</td>
    </tr>
</table>